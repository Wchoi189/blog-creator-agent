{
  "general": {
    "checkpointing": {
      "enabled": false
    }
  },
  "ui": {
    "showMemoryUsage": true
  },
  "telemetry": {
    "enabled": false
  },
  "tools": {
    "approval": {
      "mode": "auto-edit"
    },
    "allowed": [
      "run_terminal_cmd",
      "grep_search",
      "list_dir",
      "read_file",
      "search_replace",
      "create_file"
    ]
  },
  "workspace": {
    "includeDirectories": [
      "/workspaces/blog-creator-agent"
    ],
    "excludePatterns": [
      "**/node_modules/**",
      "**/.git/**",
      "**/AgentQMS/**",
      "**/.agentqms/**",
      "**/.copilot/**",
      "**/.cursor/**",
      "**/.github/**",
      "**/*.pyc",
      "**/__pycache__/**"
    ]
  },
  "agentqms": {
    "enabled": true,
    "instructions": "/workspaces/blog-creator-agent/AgentQMS/knowledge/agent/system.md",
    "architecture": "/workspaces/blog-creator-agent/.agentqms/state/architecture.yaml",
    "artifactsPath": "/workspaces/blog-creator-agent/docs/artifacts",
    "validation": {
      "enabled": false,
      "ignorePaths": [
        "docs/*.md",
        "docs/assets/**",
        "docs/archive/**",
        "AgentQMS/**",
        ".agentqms/**",
        ".copilot/**",
        ".cursor/**",
        ".github/**"
      ]
    }
  },
  "model": {
    "name": "qwen2.5-coder-32b-instruct"
  },
  "$version": 2
}